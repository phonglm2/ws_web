[
    {
      "name": "frontend",
      "links": [
        "api"
      ],
      "image": "phonglm2/wercker-frontend",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5001,
          "hostPort": 80
        }
      ],
      "memory": 500,
      "cpu": 10
    },
    {
      "name": "api",
      "image": "wercker/api",
      "cpu": 10,
      "memory": 500,
      "essential": true,
      "links": [ "redis" ],
      "command": ["python", "/pipeline/source/app.py"]
    },
    {
      "name": "redis",
      "image": "redis",
      "cpu": 10,
      "memory": 500,
      "essential": true
    }
]
